{
  "spec": {
    "swagger": "2.0",
    "info": {
      "version": "1.0.1",
      "title": "Print service API"
    },
    "host": "virtserver.swaggerhub.com",
    "basePath": "/uk-gov-dft/print-service/1.0",
    "schemes": [
      "https"
    ],
    "paths": {
      "/print-batch": {
        "post": {
          "tags": [
            "print batch"
          ],
          "summary": "Creates a batch of badges to be send for printing",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "body",
              "name": "batch",
              "description": "Batch badges to be send for printing",
              "schema": {
                "$ref": "#/definitions/Batch"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK."
            },
            "400": {
              "description": "Invalid request",
              "schema": {
                "$ref": "#/definitions/CommonResponse"
              }
            }
          }
        }
      }
    },
    "definitions": {
      "Batches": {
        "type": "array",
        "items": {
          "$ref": "#/definitions/Batch"
        }
      },
      "Batch": {
        "type": "object",
        "properties": {
          "filename": {
            "type": "string"
          },
          "batchType": {
            "type": "string",
            "example": "FASTTRACK, STANDARD or LA"
          },
          "badges": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/Badge"
            }
          }
        }
      },
      "Badge": {
        "type": "object",
        "properties": {
          "localAuthorityShortCode" : {
            "type" : "string",
            "example" : "ABERD",
            "pattern" : "^[A-Z]+$",
            "description" : "Short code of local authority."
          },
          "badgeNumber": {
            "type": "string",
            "example": "091215",
            "description": "The unique badge number for this badge."
          },
          "party": {
            "$ref": "#/definitions/Party"
          },
          "startDate": {
            "type": "string",
            "format": "date",
            "description": "The date that the badge comes into effect.",
            "example": "2018-07-07"
          },
          "expiryDate": {
            "type": "string",
            "format": "date",
            "description": "The date the badge expires.",
            "example": "2019-06-31"
          },
          "deliverToCode": {
            "type": "string",
            "maxLength": 10,
            "example": "HOME",
            "description": "A short code from the DELIVER group of reference data."
          },
          "deliveryOptionCode": {
            "type": "string",
            "maxLength": 10,
            "example": "STAND",
            "description": "A short code from the DELOP group of reference data. e.g. STAND or FAST"
          },
          "imageLink": {
            "type": "string",
            "maxLength": 255,
            "description": "A URL for the badge photo.",
            "example": "http://tiny.url?q=ab63fg"
          }
        }
      },
      "Party": {
        "type": "object",
        "properties": {
          "typeCode": {
            "type": "string",
            "description": "A short code from the PARTY group of reference data. e.g. PERSON or ORG.",
            "maxLength": 10,
            "example": "PERSON"
          },
          "contact": {
            "$ref": "#/definitions/Contact"
          },
          "person": {
            "$ref": "#/definitions/Person"
          },
          "organisation": {
            "$ref": "#/definitions/Organisation"
          }
        },
        "required": [
          "typeCode",
          "contact"
        ]
      },
      "Person": {
        "type": "object",
        "properties": {
          "badgeHolderName": {
            "type": "string",
            "maxLength": 100,
            "example": "Dwight Appleman"
          },
          "dob": {
            "type": "string",
            "format": "date",
            "example": "1972-09-13",
            "description": "Date of birth YYYY-MM-DD"
          },
          "genderCode": {
            "type": "string",
            "example": "MALE",
            "description": "A short code from the GENDER group of reference data. e.g. MALE, FEMALE or UNSPECIFIED."
          }
        },
        "required": [
          "badgeHolderName"
        ],
        "example": {
          "badgeHolderName": "Fred Bloggs",
          "dob": "1972-09-12",
          "genderCode": "MALE"
        }
      },
      "Organisation": {
        "type": "object",
        "properties": {
          "badgeHolderName": {
            "type": "string"
          }
        },
        "required": [
          "badgeHolderName"
        ],
        "example": {
          "badgeHolderName": "The Monroe Institute"
        }
      },
      "Contact": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "maxLength": 100,
            "description": "The administrative contact for the badge(s).",
            "example": "June Whitfield"
          },
          "buildingStreet": {
            "type": "string",
            "maxLength": 100,
            "example": "65 Basil Chambers"
          },
          "line2": {
            "type": "string",
            "maxLength": 100,
            "example": "Northern Quarter"
          },
          "townCity": {
            "type": "string",
            "maxLength": 100,
            "example": "Manchester"
          },
          "postCode": {
            "type": "string",
            "example": "SK6 8GH",
            "pattern": "^[A-Za-z]{1,2}[0-9][0-9A-Za-z]?\\s?[0-9][A-Za-z]{2}$"
          },
          "primaryPhoneNumber": {
            "type": "string",
            "maxLength": 20,
            "example": "01616548765"
          },
          "secondaryPhoneNumber": {
            "type": "string",
            "maxLength": 20,
            "example": "01616548765"
          },
          "emailAddress": {
            "type": "string",
            "pattern": "^\\S+\\@\\S+",
            "maxLength": 100,
            "example": "june@bigbrainknitting.com"
          }
        },
        "required": [
          "buildingStreet",
          "townCity",
          "postCode",
          "primaryPhoneNumber"
        ]
      },
      "CommonResponse": {
        "type": "object",
        "properties": {
          "apiVersion": {
            "type": "string"
          },
          "context": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "method": {
            "type": "string"
          },
          "error": {
            "$ref": "#/definitions/Error"
          }
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          },
          "errors": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/Error_errors"
            }
          }
        }
      },
      "Error_errors": {
        "properties": {
          "field": {
            "type": "string"
          },
          "reason": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "location": {
            "type": "string"
          },
          "locationType": {
            "type": "string"
          }
        }
      }
    }
  },
  "options": {
    "java8": true,
    "sortParamsByRequiredFlag": false,
    "modelPackage": "uk.gov.dft.bluebadge.model.printservice.generated",
    "apiPackage": "uk.gov.dft.bluebadge.service.printservice.generated.controller",
    "groupId": "uk.gov.dft.bluebadge",
    "artifactId": "print-service",
    "artifactVersion": "1.0.0-SNAPSHOT",
    "sourceFolder": "generated/src/main/java",
    "dateLibrary": "java8-localdatetime",
    "delegatePattern": false,
    "title": "process-sample",
    "configPackage": "uk.gov.dft.bluebadge.service.printservice.generated.config.swagger",
    "basePackage": "uk.gov.dft.bluebadge.dontneed.printservice.generated.application",
    "useBeanValidation": true,
    "performBeanValidation": true,
    "useOptional": false,
    "useTags": true,
    "hideGenerationTimestamp": true,
    "appVersion": "1.0"
  }
}
